{
    "name": "kartavik/whitebit-api",
    "type": "library",
    "description": "WhiteBIT API integration",
    "license": "MIT",
    "authors": [
        {
            "name": "Kartav1K",
            "email": "roman.varkuta@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "php-http/httplug": "^2.2",
        "php-http/message-factory": "^1.0",
        "thecodingmachine/safe": "^1.0",
        "myclabs/php-enum": "^1.6",
        "doctrine/collections": "^1.6",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "guzzlehttp/guzzle": "^7.3",
        "symfony/var-dumper": "^5.2",
        "vimeo/psalm": "^4.7",
        "phpstan/phpstan": "^0.12.85",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Kartavik\\WhiteBIT\\Api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kartavik\\WhiteBIT\\Api\\Tests\\": "tests",
            "Kartavik\\WhiteBIT\\Api\\Adapter\\": "adapter"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "phpcbf": "phpcbf --standard=PSR12 ./src"
    }
}
